    "#modifying array values\n",
    "import numpy as np\n",
    "\n",
    "a=np.arange(12).reshape(3,4)\n",
    "\n",
    "print(\"original array is\",a)\n",
    "\n",
    "for x in np.nditer(a,op_flags = ['readwrite']):\n",
    "    x[...]=x*x \n",
    "\n",
    "print(\"modified array is\",a)"
   ]
